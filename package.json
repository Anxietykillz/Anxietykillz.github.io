{
	"name": "anxietykillz.github.io.git",
	"version": "1.0.0",
	"description": "Reposition for anxietykillz.com using 11ty to build the static site.",
	"main": "index.js",
	"scripts": {
		"eleventy": "npx eleventy",
		"watch:eleventy": "eleventy --serve",
		"sass-start": "sass _src/sass/style.scss _site/assets/style.css",
		"sass-build": "sass --style=compressed _src/sass/style.scss _site/assets/style.css",
		"watch:sass": "npm run sass-start -- --watch",
		"start": "npm-run-all sass-start --parallel watch:*",
		"build": "npm-run-all sass-build --parallel eleventy"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Anxietykillz/Anxietykillz.github.io.git"
	},
	"keywords": [
		"metal",
		"health",
		"yoga",
		"accessibility",
		"mindfullness"
	],
	"author": "",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/Anxietykillz/Anxietykillz.github.io/issues"
	},
	"homepage": "https://github.com/Anxietykillz/Anxietykillz.github.io#readme",
	"devDependencies": {
		"@11ty/eleventy": "^1.0.0",
		"npm-run-all": "^4.1.5",
		"sass": "^1.49.9"
	}
}